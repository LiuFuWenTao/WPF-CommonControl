<UserControl x:Class="代码测试.创意控件.LoadingButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:代码测试.创意控件"
             xmlns:uc2="clr-namespace:代码测试.创意控件"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <uc2:Loading x:Name="loading" Width="100" Height="100" Visibility="Collapsed"></uc2:Loading>
        <uc2:LoadingComplete x:Name="loadingComplete" Width="100" Height="100" Visibility="Visible"></uc2:LoadingComplete>
    </Grid>
</UserControl>

<UserControl x:Class="WpfCommondControlApp.创意控件.MydoubleEllipseControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfCommondControlApp.创意控件"
             mc:Ignorable="d" d:DesignHeight="20" d:DesignWidth="20">
    <Grid>
        <Ellipse x:Name="MyEllipse" Stroke="#38ff82" Height="10" Width="10" Margin="5,5,0,0" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <!--<Ellipse x:Name="MyEllips1e" Stroke="#38ff82" Height="20" Width="20" Margin="0,0,0,0" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"/>-->
        <Path x:Name="MyPath1" Stroke="#38ff82" Data="M 10,0.5 A 9,9 0 0 1 19.2,8"
            StrokeDashCap="Round" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="20"/>
        <Path x:Name="MyPath2" Stroke="#1cc2eb" Data="M 19.2,12.5 A 9.5,9.5 0 0 1 8,19.3"
            StrokeDashCap="Round" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="20"/>
        <Path x:Name="MyPath3" Stroke="#1cc2eb" Data="M 3.5,17 A 8.2,9 0 0 1 3.5,3"
            StrokeDashCap="Round" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="20"/>

    </Grid>
</UserControl>